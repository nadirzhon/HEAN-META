# ========================================
# ENVIRONMENT CONFIGURATION
# ========================================
# Environment: development or production
# This controls CORS, logging, and security settings
ENVIRONMENT=development

# ========================================
# BYBIT API (REQUIRED)
# ========================================
# Get your testnet API keys from: https://testnet.bybit.com/
BYBIT_API_KEY=your-bybit-testnet-api-key
BYBIT_API_SECRET=your-bybit-testnet-api-secret
BYBIT_TESTNET=true

# Live trading confirmation (set to YES to enable order execution)
LIVE_CONFIRM=YES

# ========================================
# CAPITAL SETTINGS
# ========================================
INITIAL_CAPITAL=300

# ========================================
# AI API KEYS (OPTIONAL)
# ========================================
# Anthropic API Key for Brain analysis module
# Get your key from: https://console.anthropic.com/
# ANTHROPIC_API_KEY=your-anthropic-key-here

# Brain module settings
BRAIN_ENABLED=true
BRAIN_ANALYSIS_INTERVAL=30

# Ollama Sentiment (local LLM, free, no API key required - runs alongside Brain)
# Install Ollama: https://ollama.ai/
# Run: ollama pull llama3.2:3b
OLLAMA_ENABLED=true
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=llama3.2:3b
OLLAMA_SENTIMENT_INTERVAL=300

# RL Risk Manager (PPO-based dynamic risk adjustment)
# Falls back to rule-based logic if no trained model provided
RL_RISK_ENABLED=true
# RL_RISK_MODEL_PATH=  # Optional: path to trained PPO model (.zip)
RL_RISK_ADJUST_INTERVAL=60

# Service source selection:
# - local: in-process modules
# - redis: consume external microservice streams
PHYSICS_SOURCE=local
BRAIN_SOURCE=local
RISK_SOURCE=local
MICROSERVICES_GROUP_PREFIX=hean-core

# OpenAI API Key for agent generation
# Get your key from: https://platform.openai.com/api-keys
# OPENAI_API_KEY=your-openai-api-key-here

# ========================================
# STRATEGY SETTINGS
# ========================================
FUNDING_HARVESTER_ENABLED=true
BASIS_ARBITRAGE_ENABLED=true
IMPULSE_ENGINE_ENABLED=true

# Dormant Strategies (NOW ENABLED for maximum trading activity)
HF_SCALPING_ENABLED=true
ENHANCED_GRID_ENABLED=true
MOMENTUM_TRADER_ENABLED=true
INVENTORY_NEUTRAL_MM_ENABLED=true
CORRELATION_ARB_ENABLED=true
REBATE_FARMER_ENABLED=true
SENTIMENT_STRATEGY_ENABLED=true
LIQUIDITY_SWEEP_ENABLED=true

# Multi-Symbol Trading (scans all 50 symbols for maximum opportunities)
MULTI_SYMBOL_ENABLED=true

# ========================================
# ADVANCED EXECUTION & PHYSICS
# ========================================
# TWAP execution for large orders
TWAP_ENABLED=true
TWAP_THRESHOLD_USD=500
TWAP_DURATION_SEC=300
TWAP_NUM_SLICES=10

# Smart order type selection (limit vs market based on edge)
SMART_ORDER_SELECTION_ENABLED=true

# Physics-based position sizing and signal filtering
PHYSICS_SIZING_ENABLED=true
PHYSICS_FILTER_ENABLED=true
PHYSICS_FILTER_STRICT=true

# ========================================
# SYMBIONT X GA OPTIMIZATION
# ========================================
SYMBIONT_X_ENABLED=true
SYMBIONT_X_GENERATIONS=50
SYMBIONT_X_POPULATION_SIZE=20
SYMBIONT_X_MUTATION_RATE=0.1
SYMBIONT_X_REOPTIMIZE_INTERVAL=3600

# ========================================
# AI/ML FEATURES (ALREADY ENABLED)
# ========================================
ORACLE_DYNAMIC_WEIGHTING=true
PHYSICS_AWARE_SIZING=true
STRATEGY_CAPITAL_ALLOCATION=true
CAPITAL_ALLOCATION_METHOD=hybrid
AI_FACTORY_ENABLED=true
COUNCIL_ENABLED=true
COUNCIL_REVIEW_INTERVAL=21600
COUNCIL_AUTO_APPLY_SAFE=true

# ========================================
# PHASE 5 FEATURES
# ========================================
PHASE5_CORRELATION_ENGINE_ENABLED=true
PHASE5_SAFETY_NET_ENABLED=true
PHASE5_SELF_HEALING_ENABLED=true
PHASE5_KELLY_CRITERION_ENABLED=true
PHASE5_KELLY_FRACTIONAL=0.25

# ========================================
# REDIS (for Docker deployments)
# ========================================
# REDIS_URL=redis://localhost:6379/0
