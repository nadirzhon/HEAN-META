# HEAN SYMBIONT X Configuration File
# Bybit Testnet API Credentials

# Bybit Testnet API Keys
BYBIT_API_KEY=your_bybit_testnet_api_key_here
BYBIT_API_SECRET=your_bybit_testnet_api_secret_here
BYBIT_TESTNET=true

# Trading Configuration
INITIAL_CAPITAL=10000
SYMBOLS=BTCUSDT,ETHUSDT
MAX_LEVERAGE=2

# Risk Management
MAX_POSITION_SIZE=1000
MAX_DAILY_LOSS=500
STOP_LOSS_PCT=2.0
TAKE_PROFIT_PCT=5.0

# Strategy Parameters
POPULATION_SIZE=50
MUTATION_RATE=0.1
CROSSOVER_RATE=0.7
SELECTION_METHOD=tournament

# Regime Brain
REGIME_DETECTION_PERIOD=20
REGIME_CONFIDENCE_THRESHOLD=0.7

# Immune System
CIRCUIT_BREAKER_THRESHOLD=0.05
MAX_DRAWDOWN_PCT=10.0
HEALTH_CHECK_INTERVAL=60

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/symbiont_x.log

# Backtesting (when using historical data)
BACKTEST_MODE=false
HISTORICAL_DATA_PATH=data/historical/

# Trading Mode (Bybit Testnet - NO paper simulation)
# System uses real Bybit testnet API for all trades
# PAPER_TRADING removed - deprecated, testnet is the actual trading environment
BYBIT_TESTNET_MODE=true
