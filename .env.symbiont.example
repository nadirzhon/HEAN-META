# ============================================================================
# HEAN SYMBIONT X Configuration Template
# ============================================================================
# IMPORTANT: Copy this file to .env.symbiont and fill in your values
# NEVER commit .env.symbiont with real API keys to git!
# ============================================================================

# ============================================================================
# Bybit Testnet API Credentials
# ============================================================================
# Get your testnet API keys from: https://testnet.bybit.com/app/user/api-management
# These are TESTNET keys only - no real funds at risk
BYBIT_API_KEY=your_testnet_api_key_here
BYBIT_API_SECRET=your_testnet_api_secret_here
BYBIT_TESTNET=true

# ============================================================================
# Trading Configuration
# ============================================================================
INITIAL_CAPITAL=10000
SYMBOLS=BTCUSDT,ETHUSDT
MAX_LEVERAGE=2

# ============================================================================
# Risk Management
# ============================================================================
MAX_POSITION_SIZE=1000
MAX_DAILY_LOSS=500
STOP_LOSS_PCT=2.0
TAKE_PROFIT_PCT=5.0

# ============================================================================
# Strategy Parameters (Genetic Algorithm)
# ============================================================================
POPULATION_SIZE=50
MUTATION_RATE=0.1
CROSSOVER_RATE=0.7
SELECTION_METHOD=tournament

# ============================================================================
# Regime Brain (Market Condition Detection)
# ============================================================================
REGIME_DETECTION_PERIOD=20
REGIME_CONFIDENCE_THRESHOLD=0.7

# ============================================================================
# Immune System (Circuit Breakers & Safety)
# ============================================================================
CIRCUIT_BREAKER_THRESHOLD=0.05
MAX_DRAWDOWN_PCT=10.0
HEALTH_CHECK_INTERVAL=60

# ============================================================================
# Logging
# ============================================================================
LOG_LEVEL=INFO
LOG_FILE=logs/symbiont_x.log

# ============================================================================
# Backtesting (Optional)
# ============================================================================
# Set to true when running historical backtests (no real API calls)
BACKTEST_MODE=false
HISTORICAL_DATA_PATH=data/historical/

# ============================================================================
# Trading Mode
# ============================================================================
# System uses real Bybit testnet API for all trades
# PAPER_TRADING is deprecated - testnet is the actual trading environment
BYBIT_TESTNET_MODE=true
